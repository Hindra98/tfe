{% extends 'base.html.twig' %}

{% block title %} Inscription - FI {% endblock %}

{% block body %}
	<div
		class="w-50 mx-auto inscription">
		{# {{ form(form) }} #}
		{{ form_start(form) }}

    {{ form_row(form.nom, {label: ' ', attr: {placeholder: 'Nom'}}) }}
    {{ form_row(form.email, {label: '@', row_attr: {class: 'input-group'}, attr: {placeholder: 'Email'}}) }}
    {{ form_row(form.login, {label: ' ', attr: {placeholder: 'Login'}}) }}
    {{ form_row(form.password, {label: 'p', row_attr: {class: 'input-group'}, attr:{ placeholder: 'Mot de passe'}}) }}
    {{ form_row(form.valider, {attr: {class: 'w-100 bg-primary my-3 fw-bold'}}) }}


		{{ form_rest(form) }}

		{# {{ form_end(form) }} #}


	</div>
{% endblock %}
